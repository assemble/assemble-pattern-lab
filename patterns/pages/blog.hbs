<div class="page" id="page">
	{{ organism 'header' }}
	<div role="main">
		<h1 class="section-title">Our Outdoor Blog</h1>
		<div class="l-two-col">
			<div class="l-main">
				<section class="section latest-posts">

					<ul class="post-list">
						{{#latest-posts}}
							<li>{{ molecule 'media-block' }}</li>
						{{/latest-posts}}
					</ul>
					<a href="#" class="text-btn">View more posts</a>
				</section>
				{{ molecule 'pagination' }}
			</div><!--end l-main-->

			<div class="l-sidebar">
				{{ organism 'recent-tweets' }}
			</div><!--end l-sidebar-->
		</div><!--end two-col-->
	</div><!--End role=main-->
	{{ organism 'footer' }}
</div>